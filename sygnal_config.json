{
    "max-evalue": 10.0,
    "rho-cut": 0.3,
    "pvalue-cut": 0.05,
    "perc-targets": 0.1,
    "leo-nb-atob": 0.5,
    "mlogp-m-atob": 0.05,

    "run-neo": false,

    "tumors": ["ACC", "BLCA", "BRCA", "CESC", "CHOL", "COAD", "DLBC", "ESCA", "GBM", "HNSC", "KICH", "KIRC", "KIRP", "LGG", "LIHC", "LUAD", "LUSC", "MESO", "OV", "PAAD", "PCPG", "PRAD", "READ", "SARC", "SKCM", "STAD", "TGCT", "THCA", "THYM", "UCEC", "UCS", "UVM"],
    "synonyms-file": "/proj/omics4tb/cplaisier/cMonkey/panCancer/commonFiles/synonymsThesaurus.csv.gz",
    "phenotypes-file": "extras/phenotypes.csv",
    "ratios-file": ["/proj/omics4tb/cplaisier/cMonkey/panCancer/exprs/", "_RNAseq_medianFiltered.tsv"],
    "all-ratios-file": ["/proj/omics4tb/cplaisier/cMonkey/panCancer/exprs_all/", "_RNAseq.csv"],
    "mirna-fasta-file": "miRNA/hsa.mature.fa",
    "rand_pssms_dir": "randPSSMs",
    "promoterSeq": "seqs/promoterSeqs_Homo_sapiens.csv.gz",
    "p3utrSeq": "seqs/p3utrSeqs_Homo_sapiens.csv.gz",
    "gene_conv": "extras/entrez2ucsc.csv",

    "replication-dataset-names": ["French", "REMBRANDT", "GSE7696"],

    "meme":  { "upstream": {
                    "nmotifs": 2,
                    "motif-widths": [6, 12],
                    "revcomp": "True",
                    "bgfile": "seqs/bgFile.meme"
                }
            },
    
    "weeder": { "upstream": {
                    "bgfile": "HS",
                    "size": "small",
                    "enriched": "T50",
                    "revcomp": "True"
                },
                "3pUTR": {
                    "bgfile": "HS3P",
                    "size": "small",
                    "enriched": "T50",
                    "revcomp": "False"
                }
            },

    "tomtom": { "upstream":
                {
                    "motif-files": ["motifs/jasparCoreVertebrata_redundant.json",
                            "motifs/transfac_2012.1_PSSMs_vertabrate.json",
                            "motifs/uniprobePSSMsNonRedundant.json",
                            "motifs/selexPSSMsNonRedundant.json"]
                }
            },
    
    "tfExpansion": { "tfs": "TF/humanTFs_All.csv",
                     "tfFamilies": "TF/tfFamilies.csv"
            },

    "mirvestigator": { "seedModel": [6,7,8],
                       "minor": "True",
                       "minor": "True",
                       "p5": "True",
                       "p3": "True",
                       "wobble": "False",
                       "wobbleCut": 0.25,
                       "species": "hsa"
    },
    
    "postprocessing-result-file": "postProcessed_ACC_test_pita.csv",
    "first-principal-comps-result-file": ["biclusterFirstPrincComponents_", ".csv"]
}
